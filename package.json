{
  "name": "CFHero",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android --variant developmentDebug --appIdSuffix debug",
    "android:run:debugging": "npx react-native run-android --variant developmentDebug --appIdSuffix debug",
    "android:run:staging": "npx react-native run-android --variant stagingDebug --appIdSuffix staging",
    "android:run:production": "npx react-native run-android --variant productionRelease",
    "android:assemble:staging": "cd android && ./gradlew assembleStagingRelease",
    "android:assemble:production": "cd android && ./gradlew assembleProductionRelease",
    "android:bundle:staging": "cd android && ./gradlew bundleStagingRelease",
    "android:debugging:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "dtx:build": "detox build -c ios.sim.debug",
    "dtx:test": "detox test -c ios.sim.debug",
    "ios": "npx react-native run-ios",
    "iosd": "REACT_DEBUGGER=\"open -g 'rndebugger://set-debugger-loc?port=8081' ||\" yarn run ios --simulator=\"iPhone SE (1st generation)\"",
    "start": "npx react-native start",
    "test": "jest",
    "test:ci": "CI=true && yarn test --no-watch",
    "test:coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --no-watch",
    "lint": "yarn run tsc:check && yarn run eslint",
    "eslint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "tsc:check": "tsc --noEmit",
    "purge-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start -- --reset-cache",
    "devsetup": "cp .env.sample .env; cp .env.sample .env.development",
    "clean": "react-native-clean-project",
    "postinstall": "patch-package",
    "import-translations": "cd translations/scripts && yarn start",
    "convert-to-webp": "./utils-scripts/png2webp.sh"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix --max-warnings=0",
      "git add"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@notifee/react-native": "^4.0.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^8.3.0",
    "@react-native-firebase/analytics": "^14.5.1",
    "@react-native-firebase/app": "^14.9.4",
    "@react-native-firebase/crashlytics": "^14.5.1",
    "@react-native-firebase/firestore": "^14.5.1",
    "@react-native-firebase/storage": "^14.9.4",
    "@types/redux-persist": "^4.3.1",
    "@types/uuid": "^8.3.1",
    "d3-shape": "^3.0.1",
    "debounce": "^1.2.1",
    "deepmerge": "^4.2.2",
    "email-validator": "^2.0.4",
    "expo": "^43.0.1",
    "expo-keep-awake": "~10.0.0",
    "i18next": "^20.3.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "install": "^0.13.0",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^4.1.3",
    "memoize-one": "^5.2.1",
    "react": "17.0.1",
    "react-i18next": "^11.11.1",
    "react-native": "0.64.2",
    "react-native-bootsplash": "^4.0.0",
    "react-native-circular-progress": "^1.3.7",
    "react-native-config": "^1.4.5",
    "react-native-date-picker": "^4.1.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.0",
    "react-native-iphone-se-helper": "^0.0.2",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.1.1",
    "react-native-prompt": "^1.0.0",
    "react-native-render-html": "^4.2.4",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-svg": "^12.1.1",
    "react-native-testfairy": "^2.58.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-webp-format": "^1.1.2",
    "react-native-webview": "^10.9.3",
    "react-navigation": "^4.4.4",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.4",
    "react-swipeable-views-native": "^0.13.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "rmc-picker": "^5.0.10",
    "styled-components": "^4.4.1",
    "typesafe-actions": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/chai": "^4.2.21",
    "@types/d3-shape": "^3.0.2",
    "@types/debounce": "^1.2.1",
    "@types/detox": "^14.5.1",
    "@types/jasmine": "^3.7.8",
    "@types/jest": "^24.0.18",
    "@types/react": "^17.0.1",
    "@types/react-native": "^0.63.52",
    "@types/react-redux": "^7.1.17",
    "@types/react-test-renderer": "16.9.0",
    "@types/styled-components": "^4.1.13",
    "@typescript-eslint/parser": "^2.6.1",
    "automation-selectors": "^1.1.6",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "concurrently": "^5.0.0",
    "detox": "^16.0.0",
    "detox-getprops": "^0.1.1",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.4.0",
    "expo-cli": "^4.12.10",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^9.4.2",
    "metro-react-native-babel-preset": "^0.66.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-native-clean-project": "^3.6.4",
    "react-test-renderer": "17.0.2",
    "typescript": "3.8.3"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "./e2e/jest.config.js",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/CFHero.app",
        "build": "xcodebuild -workspace ios/CFHero.xcworkspace -scheme CFHero Dev -configuration Development -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro Max"
      }
    }
  }
}
